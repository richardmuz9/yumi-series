{
  "name": "yumi-website-builder",
  "productName": "Yumi Series - AI Platform",
  "description": "Advanced AI platform for content creation, web building, and character design",
  "private": true,
  "version": "1.3.2",
  "author": {
    "name": "Yumi AI",
    "email": "support@yumi.ai",
    "url": "https://yumi77965.online"
  },
  "homepage": "https://yumi77965.online",
  "main": "electron-main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "predeploy": "node scripts/check-env.js",
    "deploy": "npm run build",
    "preview": "vite preview",
    "start:web": "vite",
    "start:desktop": "concurrently \"npm run build:watch\" \"wait-on http://localhost:5173 && electron .\"",
    "build:web": "vite build",
    "build:watch": "vite build --watch",
    "build:desktop": "npm run build:web && electron-builder",
    "package:desktop": "npm run build:web && electron-builder --publish=never",
    "package:all": "npm run build:web && electron-builder --mac --win --linux",
    "dist": "npm run build:web && electron-builder --publish=always",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "build:mobile": "npm run build:web && npx cap sync",
    "dev:mobile": "npm run build:web && npx cap sync && npx cap copy",
    "android:open": "npx cap open android",
    "ios:open": "npx cap open ios",
    "android:run": "npm run build:mobile && npx cap run android",
    "ios:run": "npm run build:mobile && npx cap run ios",
    "mobile:sync": "npx cap sync",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@capacitor/android": "^5.5.0",
    "@capacitor/cli": "^5.5.0",
    "@capacitor/core": "^5.5.0",
    "@capacitor/ios": "^5.5.0",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "@types/katex": "^0.16.7",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "astral-regex": "^2.0.0",
    "axios": "^1.6.7",
    "chart.js": "^4.5.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "katex": "^0.16.22",
    "lucide-react": "^0.330.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-easy-crop": "^5.0.4",
    "react-grid-layout": "^1.4.4",
    "react-i18next": "^15.5.3",
    "react-icons": "^5.0.1",
    "react-katex": "^3.1.0",
    "react-router-dom": "^6.22.0",
    "sirv": "^2.0.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.17",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-katex": "^3.0.4",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "debug": "^4.4.1",
    "electron": "^28.2.3",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-pwa": "^0.17.4",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.yumiai.yumiseries",
    "productName": "Yumi Series",
    "artifactName": "${productName}-${version}-${arch}.${ext}",
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron-main.js",
      "electron-preload.js",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "public/pwa-512.svg",
        "to": "icon.svg"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/pwa-512.svg",
      "publisherName": "Yumi AI",
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Yumi Series"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "public/pwa-512.svg",
      "category": "public.app-category.productivity",
      "darkModeSupport": true
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "snap",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/pwa-512.svg",
      "category": "Office"
    },
    "publish": {
      "provider": "github",
      "owner": "yumi-ai",
      "repo": "yumi-series",
      "private": false
    }
  }
}
